(custom-set-typed-variable (quote default-font) (quote ("Pango" . "Raleway Medium 10")) (quote font))
(custom-set-typed-variable (quote SawthenaForever:active-text-color) (quote "#ed00eb00d300") (quote (optional color)))
(custom-set-typed-variable (quote SawthenaForever:normal-color) (quote "#d900c500a100") (quote (optional color)))
